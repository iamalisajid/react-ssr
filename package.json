{
  "author": "Ali Sajid",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build": "webpack --mode production",
    "lint": "esw",
    "lint:watch": "esw --watch",
    "format": "prettier --config .prettierrc --write 'src/**/*.{js,jsx}'",
    "format:watch": "onchange '**/*.js' -- yarn format",
    "lint:css": "stylelint './src/**/*.js'",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-knobs": "^4.0.4",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "axios": "^0.18.0",
    "change-case-object": "^2.0.0",
    "express": "^4.16.4",
    "json-server": "^0.14.0",
    "lodash": "latest",
    "node-sass": "^4.9.4",
    "onchange": "^5.0.2",
    "prop-types": "latest",
    "qs": "^6.5.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-loader-spinner": "^2.0.6",
    "react-redux": "^5.1.0",
    "react-router": "latest",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-form": "^7.4.2",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^4.0.3",
    "styled-theming": "^2.2.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "assets-webpack-plugin": "^3.9.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.3",
    "node-sass": "^4.10.0",
    "onchange": "^5.0.2",
    "prettier": "^1.14.3",
    "react-hot-loader": "^4.3.12",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.7.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-messages": "^2.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ],
    "**/*.{css,scss}": [
      "stylelint --syntax scss",
      "git add"
    ]
  }
}
